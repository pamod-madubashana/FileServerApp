{
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "http:allow-fetch",
    "shell:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://*" },
        { "url": "https://*" }
      ]
    }
  ]
}